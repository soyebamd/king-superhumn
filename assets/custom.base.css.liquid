:root {
  --font-heading-family: "PhonkContrast";
  --color-primary:#ea3323;
}

.text-primary {
  color: var(--color-primary) !important;
}

body{
  font-family: "neue_haas";
}

h1,
h2,
h3,
h4,
h5,
h6,
.h1,
.h2,
.h3,
.h4,
.h5,
.h6 {
  font-family: "Zuume";

font-weight:900;

font-style:italic;

}


.hxxl{font-size:230px;}

.hxl{ font-size: 170px;} 

.h0{
   font-size: 136px;
}

h1,
.h1 {
   font-size: 83px;
}

h2,
.h2 {
   font-size: 58px;
}

h3,
.h3 {
 font-size: 42px;
}


p{
font-size: 28px;
font-style: normal;
font-weight: 400;
font-family: "neue_haas";
line-height: 34px; /* 121.429% */
 }


.caption-text {
  letter-spacing: 0.831px;
  margin:0;
  font-style:normal;
}
mark {
  -webkit-text-stroke-width: 2px;
  -webkit-text-stroke-color: var(--color-primary);
  text-stroke-width: 2px;
  text-stroke-color: var(--color-primary);
font-weight:900;
 font-style:italic;
  background: transparent;
 
}

:root{

  --color-voilet:#931cf5;
  
}

.superhumn-image-banner .banner__text{
  max-width:710px;
}

.banner__content.caption-wrapper{
  padding:60px!important;
}

.banner__buttons{
  max-width:60rem!important;
}

.button{
    display: inline-flex;
    min-width: 247px;
    height: 64px;
    padding: 0;
    justify-content: center;
    align-items: center;
    gap: 10px;
   
    font-size: 28px;
    font-style: normal;
    font-weight: 700;
    line-height: 64px;
     font-family: "Zuume";
}

.button--secondary{
  background:var(--color-primary);
  color:#fff;
  border-color:var(--color-primary);
}

.button--secondary:after{
  box-shadow:none!important;
}


.caption-bar {
  position: relative;
}

         .caption-graphic   {
                        width: 69px;
  display: block;
  position: absolute;
  left: -70px;
  top: 0;
}




                  .caption-graphic img{   display: inline-block;
  margin: 0 2px;}
                 

.list-menu {
  font-family: "PhonkContrast";
}

.list-menu a {
  font-size: 2.6rem;
  color: #fff;
}

.full-width {
  width: 100% !important;
  max-width: 100% !important;
}

.header__menu-item {
  padding: 1.6rem;
}

.page-width-full {
  width: 100%;
  max-width: 100%;
}

.page-width-medium{
  max-width:calc(1440px + 10rem);
}


.position-relative{
  position:relative;
}


/* min device off 990px */
@media screen and (min-width: 990px) {
  .header--middle-left {
    grid-template-columns: auto;
  }
  .header__inline-menu {
    text-align: center;
  }
  header.header {
    padding-top: 1.6rem;
    padding-bottom: 1.6rem;
  }
}



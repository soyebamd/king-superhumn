/* min device off 990px */
@media screen and (min-width: 990px) {
  .header--middle-left {
    grid-template-columns: auto;
  }
  .header__inline-menu {
    text-align: center;
  }
  header.header {
    padding-top: 1.6rem;
    padding-bottom: 1.6rem;
  }
}



.text-behind-shadow {
       content: "";
    position: absolute;
    border-radius: 20px;
    background: #000;
    filter: blur(150px);
    display: block!important;
    max-width: 1191px;
    height: 630px;
    z-index: 1;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    width: 100%;
}

  .team-feature-img{
    position:absolute;

     left:-100%;

  transition:all 1s;
  animation: myAnim 10s ease 0s infinite normal forwards;


  }

.feature-img{
  position:absolute;

  right:-100%;

  transition:all 1s;
  animation: myAnim 10s ease 0s infinite normal forwards;
}


.character{
 position: absolute;
  

   right:-100%;
  top: 100px;
   transition:all 1.8s;
  animation: myAnim 14s ease 0s infinite normal forwards;
  
}

.character-from-right{

right: -120px;

}



.comeFromleft{
   left:0%;
}

.comeFromRight{
   right:0%;
}

@keyframes myAnim {
	0%,
	100% {
		transform: translateY(0) rotate(0deg);
	}

	10%,
	30%,
	50%,
	70% {
		transform: translateY(-18px) rotate(2deg);;
	}

	20%,
	40%,
	60% {
		transform: translateY(18px) rotate(0deg);;
	}

	80% {
		transform: translateY(11.4px);
	}

	90% {
		transform: translateY(-11.4px) rotate(2deg);; 
	}
}




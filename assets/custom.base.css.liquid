:root {
  --font-heading-family: "PhonkContrast";
  --color-primary:#ea3323;
}

.text-primary {
  color: var(--color-primary) !important;
}

body{
  font-family: "neue_haas";
}

h1,
h2,
h3,
h4,
h5,
h6,
.h1,
.h2,
.h3,
.h4,
.h5,
.h6 {
  font-family: "Zuume";

font-weight:900;

font-style:italic;

line-height:normal!important;

}


.hxxl{font-size:230px; }

.hxl{ font-size: 170px;} 

.h0{
   font-size: 136px;
}

h1,
.h1 {
   font-size: 83px;
}

h2,
.h2 {
   font-size: 58px;
}

h3,
.h3 {
 font-size: 42px;
}

.text-white{
  color:#fff!important;
}

.text-white a{
  color:#fff!important;
}



p{
font-size: 28px;
font-style: normal;
font-weight: 400;
font-family: "neue_haas";
line-height: 34px; /* 121.429% */
 }


.caption-text {
  letter-spacing: 0.831px;
  margin:0;
  font-style:normal;
}
mark {
  -webkit-text-stroke-width: 2px;
  -webkit-text-stroke-color: var(--color-primary);
  text-stroke-width: 2px;
  text-stroke-color: var(--color-primary);
font-weight:900;
 font-style:italic;
  background: transparent;
 
}

.img-fluid{
  max-width:100%;
}

.py-40{
  padding-top:40px;
  padding-bottom:40px;
}

:root{

  --color-voilet:#931cf5;
  
}

.superhumn-image-banner .banner__text{
  max-width:710px; margin:0 auto;
}

.banner__content.caption-wrapper{
  padding:60px!important;
}

.banner__buttons{
  max-width:60rem!important;
}

.banner__box--shadow::after{
    content: '';
    position: absolute;
    border-radius: 1191px;
    background: #000;
    filter: blur(150px);
    display: block;
    max-width: 1191px;
    height: 630px;
    z-index: 111;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
   
}



.button{
    display: inline-flex;
    min-width: 247px;
    height: 64px;
    padding: 0;
    justify-content: center;
    align-items: center;
    gap: 10px;
   
    font-size: 28px;
    font-style: normal;
    font-weight: 700;
    line-height: 64px;
     font-family: "Zuume";
}

.button--secondary{
  background:var(--color-primary);
  color:#fff;
  border-color:var(--color-primary);
}

.button--secondary:after{
  box-shadow:none!important;
}


.caption-bar {
  position: relative;
}

         .caption-graphic   {
                        width: 69px;
  display: block;
  position: absolute;
  left: -70px;
  top: 0;
}




                  .caption-graphic img{   display: inline-block;
  margin: 0 2px;}
                 

.list-menu {
    font-family: "Zuume";

}

.list-menu a {
  font-size: 3.2rem;
  color: #fff;
  font-weight:700;
}

.full-width {
  width: 100% !important;
  max-width: 100% !important;
}

.header__menu-item {
  padding: 1.6rem;
}

.page-width-full {
  width: 100%;
  max-width: 100%;
}

.page-width-medium{
  max-width:calc(1440px + 10rem);
}


.position-relative{
  position:relative;
}

@media(min-width:750px){
  .superhumn-title {
        position: relative;
    left: -58px;
    margin:0;
  }
}


/*
Product Card
*/

.icon-power {
    position: absolute;
    bottom: -34%;
    left: 10px;
    z-index: 1;
}

.contains-card li{
  border: 12px solid #FFF;
  background: #222;
}

.product-type {
  color: #b526d3 !important;
  font-family: "Zuume";
  font-size: 38px;
  font-style: normal;
  font-weight: 700;

}

.card__heading {
  line-height:normal;
}
.superhumn-card-items .price{
  display: none;
}

.superhumn-card-items .card__content{margin: 0 32px;
  width: auto;}

.p-0{
  padding:0!important;
}

.m-0{
  margin:0!important;
}

.collection__view-all {
  margin-top:30px;
}

/*Card product video style*/

.controlVd{
  max-width:100%;
  transform:scale(1.7);
}

/* min device off 990px */
@media screen and (min-width: 990px) {
  .header--middle-left {
    grid-template-columns: auto;
  }
  .header__inline-menu {
    text-align: center;
  }
  header.header {
    padding-top: 1.6rem;
    padding-bottom: 1.6rem;
  }
}


